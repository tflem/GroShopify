@model ProductsListViewModel

<table class="table table-striped table-dark">
  <thead class="thead-dark">
    <tr>
      <th>Name</th>
      <th>Quantity</th>
      <th>Actions</th>
    </tr>
  </thead>

@foreach (var p in Model.Products) {

  <tbody>   
     <td>@p.Name</td>
     <td>@p.Quantity</td>
     <td>Edit | Delete | Add More</td>                  
  </tbody>  
}
</table>